{
	"name": "postie",
	"version": "0.1.1",
	"description": "A WordPress block for fetching posts, pages and custom post types.",
	"author": "Lambry",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build": "npm run build:editor && npm run build:frontend",
		"build:editor": "wp-scripts build --output-path=build/editor",
		"build:frontend": "wp-scripts build ./src/frontend/index.js --output-path=build/frontend",
		"format:js": "wp-scripts format-js",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"start": "npm run start:editor",
		"start:editor": "wp-scripts start --output-path=build/editor",
		"start:frontend": "wp-scripts start ./src/frontend/index.js --output-path=build/frontend",
		"packages-update": "wp-scripts packages-update"
	},
	"devDependencies": {
		"@wordpress/scripts": "^19.0.0"
	},
	"dependencies": {
		"@wordpress/api-fetch": "^5.2.4",
		"@wordpress/block-editor": "^7.0.4",
		"@wordpress/blocks": "^11.1.2",
		"@wordpress/components": "^19.0.0",
		"@wordpress/data": "^6.1.2",
		"@wordpress/element": "^4.0.3",
		"@wordpress/i18n": "^4.2.3",
		"@wordpress/icons": "^6.0.1",
		"@wordpress/server-side-render": "^3.0.4",
		"swiper": "^7.2.0"
	}
}
